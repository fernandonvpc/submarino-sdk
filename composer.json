{
    "name": "gpupo/submarino-sdk",
    "description": "SDK Não Oficial para integração a partir de aplicações PHP com as APIs do Submarino Marketplace",
    "homepage": "https://opensource.gpupo.com/submarino-sdk/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["submarino","americanas", "B2W", "shoptime", "sdk", "api"],
    "license": "MIT",
    "require": {
        "ext-bcmath" : "*",
        "gpupo/common-sdk": "^4.14|dev-master"
    },
    "require-dev": {
      "phpunit/phpunit": "^8.0",
      "phan/phan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\SubmarinoSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\SubmarinoSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "bin": ["bin/submarino-sdk"],
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
